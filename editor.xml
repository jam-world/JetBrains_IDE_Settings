<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="4" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c02c00" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bin\sprogress\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <pattern useCustomColors="true" pattern="\b(done|resolved)\b.*">
      <option name="FOREGROUND" value="35400d" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="\bfeature\b.*" />
    <pattern useCustomColors="true" pattern="\bbacklog\b.*">
      <option name="FOREGROUND" value="91a923" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\breminder\b.*">
      <option name="FOREGROUND" value="c06a00" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\b(note|notice)\b.*">
      <option name="FOREGROUND" value="c09f50" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bissue\b.*">
      <option name="FOREGROUND" value="c05000" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bfixed\b.*">
      <option name="FOREGROUND" value="35400d" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="todo">
      <pattern index="1" />
      <pattern index="4" />
      <pattern index="0" />
      <pattern index="8" />
      <pattern index="2" />
      <pattern index="6" />
    </filter>
    <filter name="fixme">
      <pattern index="1" />
    </filter>
    <filter name="done">
      <pattern index="3" />
    </filter>
    <filter name="in progress">
      <pattern index="2" />
    </filter>
    <filter name="important">
      <pattern index="1" />
      <pattern index="2" />
    </filter>
    <filter name="reminders">
      <pattern index="6" />
    </filter>
  </component>
</application>